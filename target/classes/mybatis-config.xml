<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
<typeAliases> 
<!-- 회원정보 및 회원가입 Dto -->
<typeAlias type="com.spring.ex.dto.MemberDto" alias="MemberDto"/> 
<!--MemberDto를 써야하는데 기니까 MemberDto라고 alias라고 별명 --> <!--그후 Mappers에 mapper.xml-->

<!-- 상품정보 Dto -->
<typeAlias type="com.spring.ex.dto.ProductDto" alias="ProductDto"/>

<!-- 상품주문내역  Dto -->
<typeAlias type="com.spring.ex.dto.JumunDto" alias="JumunDto"/>

<!-- 장바구니  Dto -->
<typeAlias type="com.spring.ex.dto.BasketDto" alias="BasketDto"/> 

<!-- 고객문의 Dto -->
<typeAlias type="com.spring.ex.dto.BoardDTO" alias="BoardDTO"/>

<!-- 환불요청 Dto -->
<typeAlias type="com.spring.ex.dto.RefundDto" alias="RefundDto"/>
</typeAliases>
</configuration> 